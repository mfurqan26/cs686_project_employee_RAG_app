// This is your Prisma schema file,

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Business {
  id        String            @id @default(cuid())
  name      String
  industry  String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}